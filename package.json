{
  "name": "doaibu-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/setup-azure-env.js dev && npx prisma generate && next dev",
    "build": "node scripts/setup-azure-env.js build && npx prisma generate && next build",
    "build:azure": "node scripts/setup-azure-env.js azure && npx prisma generate && next build",
    "start": "node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "setup:env": "node scripts/setup-azure-env.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:seed-admin": "node scripts/seed-azure-admin.js",
    "postinstall": "node scripts/azure-startup.js && npx prisma generate"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/core-auth": "^1.9.0",
    "@azure/openai": "^2.0.0",
    "@azure/storage-blob": "^12.27.0",
    "@emailjs/browser": "^4.4.1",
    "@microsoft/signalr": "^8.0.7",
    "@prisma/client": "^6.9.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.378.0",
    "multer": "^2.0.1",
    "next": "14.2.3",
    "next-auth": "^4.24.11",
    "openai": "^5.3.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.5.2",
    "resend": "^4.6.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "prisma": "^6.9.0"
  }
}
